    "nome": {
      "type": "string"
    },
    "descricao": {
      "type": "text",
      "default": "Descrição"
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tipos_de_entrega": {
      "type": "enumeration",
      "enum": [
        "Entregar",
        "Receber"
      ]
