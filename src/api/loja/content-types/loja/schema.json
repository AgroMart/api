{
  "kind": "collectionType",
  "collectionName": "lojas",
  "info": {
    "singularName": "loja",
    "pluralName": "lojas",
    "displayName": "Loja",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
