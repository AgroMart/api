      "target": "api::endereco.endereco"
    },
    "cestas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cesta.cesta",
      "mappedBy": "loja"
    },
    "planos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::plano.plano",
      "mappedBy": "lojas"
    },
    "assinantes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::assinante.assinante",
      "mappedBy": "lojas"
    },
    "produto_avulsos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::produto-avulso.produto-avulso",
